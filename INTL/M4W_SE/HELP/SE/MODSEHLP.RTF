\par \page {\cs41\super #{\footnote \pard\plain \s36\widctlpar \f4\fs20\lang1031 {\cs41\super #} Ins_c2tun}}{\fs24\cf2  }{\b\fs28\ul\cf2 C-2 Tuning}
